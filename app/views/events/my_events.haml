%h1= t('my_events.title')
- @my_events.each do |personal_event|
  .event= link_to personal_event.name, personal_event

%h1= t('my_events.interested_in')
- if !@interested_to_attend.nil?
  - @interested_to_attend.each do |attendance|
    .event= link_to attendance.event.name, attendance.event
    .action
      = render :partial => "attendance_actions", :locals => {:attendance => attendance, :event => attendance.event}
